package com.firstwap.middleware.ejb.entity;

import java.io.Serializable;
import java.sql.Timestamp;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "SMS_DISPATCHER")
public class SmsDispatcher implements Serializable, Cloneable {

	private static final long serialVersionUID = -8971817314455581613L;
	
	private long id;
	private String tranID;
	private String src;
	private String dest;
	private String msg;
	private int statIntern;
	private int msgCntOto;
	private int msgCntFwap;
	private String delvStat;
	private Timestamp dateStampSent;
	private Timestamp dateStampDelv;
	private String statReason;
	private String costCenter;
	private Timestamp scheduleDate;
	private String webUser;
	private String apiUser;
	private int flash;
	private int msgType;
	private Date runDate;
	private String serverName;

	// getters and setters
	@Column(name = "RUN_DATE", nullable = true)
	public Date getRunDate() {
		return runDate;
	}
	public void setRunDate(Date runDate) {
		this.runDate = runDate;
	}
	
	@Column(name = "SERVER_NAME")
	public String getServerName() {
		return serverName;
	}
	public void setServerName(String serverName) {
		this.serverName = serverName;
	}
	
	@Column(name = "DATESTAMP_DELV")
	public Timestamp getDateStampDelv() {
		return dateStampDelv;
	}
	public void setDateStampDelv(Timestamp dateStampDelv) {
		this.dateStampDelv = dateStampDelv;
	}

	@Column(name = "DATESTAMP_SENT")
	public Timestamp getDateStampSent() {
		return dateStampSent;
	}
	public void setDateStampSent(Timestamp dateStampSent) {
		this.dateStampSent = dateStampSent;
	}

	@Column(name = "DELV_STAT")
	public String getDelvStat() {
		return delvStat;
	}
	public void setDelvStat(String delvStat) {
		this.delvStat = delvStat;
	}

	@Column(name = "DEST")
	public String getDest() {
		return dest;
	}
	public void setDest(String dest) {
		this.dest = dest;
	}

	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	@Column(name = "ID")
	public long getID() {
		return id;
	}
	public void setID(long id) {
		this.id = id;
	}

	@Column(name = "MSG")
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}

	@Column(name = "MSG_CNT_FWAP")
	public int getMsgCntFwap() {
		return msgCntFwap;
	}
	public void setMsgCntFwap(int msgCntFwap) {
		this.msgCntFwap = msgCntFwap;
	}

	@Column(name = "MSG_CNT_OTO")
	public int getMsgCntOto() {
		return msgCntOto;
	}
	public void setMsgCntOto(int msgCntOto) {
		this.msgCntOto = msgCntOto;
	}

	@Column(name = "SRC")
	public String getSrc() {
		return src;
	}
	public void setSrc(String src) {
		this.src = src;
	}

	@Column(name = "STAT_INTERN")
	public int getStatIntern() {
		return statIntern;
	}
	public void setStatIntern(int statIntern) {
		this.statIntern = statIntern;
	}

	@Column(name = "TRAN_ID")
	public String getTranID() {
		return tranID;
	}
	public void setTranID(String tranID) {
		this.tranID = tranID;
	}

	@Column(name = "STAT_REASON")
	public String getStatReason() {
		return statReason;
	}
	public void setStatReason(String statReason) {
		this.statReason = statReason;
	}

	@Column(name = "COST_CENTER")
	public String getCostCenter() {
		return costCenter;
	}
	public void setCostCenter(String costCenter) {
		this.costCenter = costCenter;
	}

	@Column(name = "SCHEDULE_DATE")
	public Timestamp getScheduleDate() {
		return scheduleDate;
	}
	public void setScheduleDate(Timestamp scheduleDate) {
		this.scheduleDate = scheduleDate;
	}
	
	@Column(name = "API_USER")
	public String getApiUser() {
		return apiUser;
	}
	public void setApiUser(String apiUser) {
		this.apiUser = apiUser;
	}

	@Column(name = "WEB_USER")
	public String getWebUser() {
		return webUser;
	}
	public void setWebUser(String webUser) {
		this.webUser = webUser;
	}
	
	@Column(name = "FLASH")
	public int getFlash() {
		return flash;
	}
	public void setFlash(int flash) {
		this.flash = flash;
	}
	
	@Column(name = "MSG_TYPE")
	public int getMsgType() {
		return msgType;
	}
	public void setMsgType(int msgType) {
		this.msgType = msgType;
	}
	
	@Override
	public String toString() {
		return "[servername="+getServerName()+" rundate="+getRunDate()+" id=" + getID() + ", tran_id=" + getTranID() + ", src=" + getSrc() 
			+ ", dest=" + getDest() + ", msg=$" + getMsg() + "$, cost_center=" + getCostCenter() 
			+ ", stat_intern=" + getStatIntern() + ", msg_cnt_fwap=" + getMsgCntFwap()
			+ ", delv_stat=" + getDelvStat() + ", datestamp_sent=" + getDateStampSent()
			+ ", datestamp_delv=" + getDateStampDelv() + ", schedule_date=" + getScheduleDate()
			+ ", stat_reason=" + getStatReason() + ", web_user=" + getWebUser()
			+ ", api_user=" + getApiUser() + "]";
	}
	
	@Override
	public Object clone() {
		Object clone = null;
		try {
			clone = super.clone();
		} catch (CloneNotSupportedException e) {
			e.printStackTrace();
		}
		return clone; 
	}

	/**
	 * constructor
	 * 
	 */
	public SmsDispatcher() {
	}
}
